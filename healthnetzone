`<h1>The Role of Water in Digestion</h1><p class='meta'>By HealthNetZone | Sep 26, 2025</p><div class='content'><p>Water is essential for digestion, nutrient absorption, and detoxification. Drink throughout the day for optimal health.</p></div>`
];

let page = 0;
const PAGE_SIZE = 10;

function renderFeed() {
    const start = page*PAGE_SIZE;
    const end = Math.min(ARTICLES.length, start+PAGE_SIZE);
    const area = document.getElementById('content-area');
    area.innerHTML = '';
    for(let i=start;i<end;i++){
        const post = ARTICLES[i];
        const title = post.match(/<h1>(.*?)<\/h1>/)[1];
        const meta = post.match(/<p class='meta'>(.*?)<\/p>/)[1];
        const excerptMatch = post.match(/<div class='content'>([\s\S]*?)<\/div>/);
        const excerpt = excerptMatch?excerptMatch[1].slice(0,300):'';
        const card = document.createElement('div');
        card.className='card';
        card.innerHTML = `<h3 class='post-title'>${title}</h3><div class='post-meta'>${meta}</div><div class='post-excerpt'>${excerpt}...</div><p><a href='#' class='button' onclick='openPost(${i});return false;'>Read More</a></p>`;
        area.appendChild(card);
    }
    const pag = document.createElement('div');
    if(start>0){
        const prevBtn = document.createElement('button');
        prevBtn.innerText='← Previous Page';
        prevBtn.onclick=function(){page--; renderFeed(); window.scrollTo({top:0, behavior:'smooth'});};
        pag.appendChild(prevBtn);
    }
    if(end<ARTICLES.length){
        const nextBtn = document.createElement('button');
        nextBtn.innerText='Next Page →';
        nextBtn.onclick=function(){page++; renderFeed(); window.scrollTo({top:0, behavior:'smooth'});};
        pag.appendChild(nextBtn);
    }
    area.appendChild(pag);
}

function openPost(i){
    if(i<0 || i>=ARTICLES.length) return;
    const area = document.getElementById('content-area');
    area.innerHTML='';
    const card = document.createElement('div');
    card.className='card';
    card.innerHTML=ARTICLES[i]+`<p style="margin-top:12px"><a href='#' class='button' onclick='openPost(${i-1});return false;'>← Previous</a> <a href='#' class='button' style="margin-left:8px" onclick='openPost(${i+1});return false;'>Next →</a></p>`;
    area.appendChild(card);
    window.scrollTo({top:0, behavior:'smooth'});
}

function showHome(){ renderFeed(); }

function openModal(id){ document.getElementById(id).style.display='block'; }
function closeModal(id){ document.getElementById(id).style.display='none'; }

function loadLatest(){
    const ul=document.getElementById('latest-list');
    ul.innerHTML='';
    for(let i=0;i<Math.min(10,ARTICLES.length);i++){
        const t = ARTICLES[i].match(/<h1>(.*?)<\/h1>/)[1];
        const li = document.createElement('li');
        li.innerHTML = `<a href='#' onclick='openPost(${i});return false;'>${t}</a>`;
        ul.appendChild(li);
    }
}

document.addEventListener('DOMContentLoaded',function(){
    renderFeed();
    loadLatest();
});
</script>
